<?xml version='1.0' encoding='utf-8'?>
<widget id="com.litchnom.data" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Polyphasic Tracker</name>
    <description>
	An app to get and track polyphasic sleep related data from users in the Polyphasic Sleeping Discord server
    </description>
    <author email="" href="litchnom.com">
        Litchnom
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
